<head>
	<%= stylesheet_link_tag('application', :media => 'all') %>
</head>

<div class="blue-bar">
	<p class ="top-header">Showing Group <%= @group.gname %> For User <%= currentUser.email %></p>
</div>

<div align="center" class="forms">

	<h4>Group Information</h4>
		<table class ="groups">
			<tr>
				<th>Members</th>
			</tr>
			<% @members.each do |t| %>
			<tr>
				<td><%= t.u_email %></td>
			</tr>
			<% end %>
		</table>

<br>


<%= link_to "Back to profile", currentUser %>

</div>

